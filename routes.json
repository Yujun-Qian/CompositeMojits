[
  {
    "settings": ["master"],
    "root": {
      "verbs": ["get"],
      "path": "/",
      "call": "frame.index"
    },
    "header": {
      "verbs": ["get"],
      "path": "/header",
      "call": "header.index",
      "params": "header"
    },
    "body": {
      "verbs": ["get"],
      "path": "/body",
      "call": "body.index"
    },
    "footer": {
      "verbs": ["get"],
      "path": "/footer",
      "call": "footer.index"
    },

    "any mojit/action": {
        "verbs": [ "GET" ],
        "path": "/:module/:action",
        "call": "{module}.{action}"
    }
  }
]
